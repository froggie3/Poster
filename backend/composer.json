{
    "require": {
        "monolog/monolog": "^3.4",
        "debril/feed-io": "^6.0",
        "symfony/http-client": "^7.0",
        "minicli/minicli": "^4.2",
        "guzzlehttp/guzzle": "^7.8",
        "minicli/command-help": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": ["app/src/", "app/Constants/", "app/Config/"],
            "App\\Data\\": "app/data/",
            "App\\Interface\\": "app/Interface/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
